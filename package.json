{
  "name": "mint-ui",
  "version": "0.1.12",
  "description": "Mobile UI elements for vue.js",
  "keywords": [
    "component",
    "vue",
    "mobile",
    "eleme"
  ],
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
