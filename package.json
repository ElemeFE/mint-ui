  "dependencies": {},
  "devDependencies": {
    "gh-pages": "^0.11.0",
    "kanpai": "^0.5.3",
    "lerna": "2.0.0-beta.18",
    "my-local-ip": "^1.0.0",
    "uppercamelcase": "^1.1.0",
    "vue-clickoutside": "0.0.4",
    "vue-infinite-scroll": "^0.2.3",
    "vue-lazyload": "^0.4.3",
    "vue-msgbox": "^0.2.10",
    "vue-popup": "^0.1.8",
